FoamFile
{
    version     2.0;
    format      binary;
    class       pointScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      pointLevel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
    }
    outlet
    {
        type            calculated;
    }
    farside
    {
        type            calculated;
    }
    nearside
    {
        type            calculated;
    }
    hill
    {
        type            calculated;
    }
    floor
    {
        type            calculated;
    }
    top
    {
        type            calculated;
    }
}


